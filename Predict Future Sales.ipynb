    "# clip out sales of more than 20 in a month\n",
    "clippedSales = groupSalesDf.copy()\n",
    "clippedSales['item_cnt_month'].clip(0,20, inplace=True)\n",
    "clippedSales['item_cnt_month'].max()"
   ]
  },
