   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Create Rolling Sales Data\n",
    "rollingSales = clippedSales.copy()\n",
    "for i in range(1,13):\n",
    "    rollingSales['last_%dmonth_sales' % (i)] = rollingSales.groupby(['item_id'])['item_cnt_month'].shift(i)\n",
    "    rollingSales['last_%dmonth_diff' % (i)] = rollingSales.groupby(['item_id'])['last_%dmonth_sales' % (i)].diff()"
   ]
  },
